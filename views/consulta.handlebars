<div class="container"></div>
        <h1>Tela de Consulta</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>Telefone</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody>
                {{#each posts}}
                    {{#dataValues}}
                <tr>
                    <td>{{nome}}</td>
                    <td>{{endereco}}</td>
                    <td>{{bairro}}</td>
                    <td>{{cep}}</td>
                    <td>{{cidade}}</td>
                    <td>{{estado}}</td>
                    <td>
                         <a class="btn btn-primary btn-sm" style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .2rem; --bs-btn-font-size: .68rem;" href="/editar/{{id}}">Editar</a>
                                <a class="btn btn-primary btn-sm" style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .2rem; --bs-btn-font-size: .68rem;" href="/excluir/{{id}}" >Excluir</a>
                    </td>
                </tr>
                {{/dataValues}}
            {{/each}}
            </tbody>
            
        </table>
    </div>